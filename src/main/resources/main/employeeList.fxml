<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.EmployeeListController">
    <children>
        <TableView fx:id="employeeTable" layoutX="10.0" layoutY="10.0" prefHeight="300.0" prefWidth="620.0">
            <columns>
                <TableColumn fx:id="empIDCol" prefWidth="100.0" text="ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="empID"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="firstNameCol" prefWidth="150.0" text="First Name">
                    <cellValueFactory>
                        <PropertyValueFactory property="firstName"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="lastNameCol" prefWidth="150.0" text="Last Name">
                    <cellValueFactory>
                        <PropertyValueFactory property="lastName"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="phoneNumberCol" prefWidth="150.0" text="Phone Number">
                    <cellValueFactory>
                        <PropertyValueFactory property="phoneNumber"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="employeeTypeCol" prefWidth="100.0" text="Type">
                    <cellValueFactory>
                        <PropertyValueFactory property="employeeType"/>
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <Button layoutX="10.0" layoutY="320.0" mnemonicParsing="false" text="Back" onAction="#goBack"/>
    </children>
</AnchorPane>
